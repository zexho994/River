interface River<T>{
    + (static) Stream<T> of(T...)
    + Stream<T> filter(Predicate<? super T>)
    + Stream<T> distinct();
    + forEach(Consumer<? super T>)
    + long count()
}

class RiverGenerator{
    + (static)<T> River<T> create(T...)
}

